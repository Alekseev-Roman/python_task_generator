        <![CDATA[import subprocess, sys

def generate():
    str_1 = '{}{}\n'.format('abcacba',' '*5)
    str_2 = '{}{}\n'.format(' bcba',' '*5)
    str_3 = '{}{}\n'.format('baa',' '*5)
    str_4 = '{}{}\n'.format('ccc',' '*4)
    str_5 = '{}{}\n'.format('bbb',' '*4)
    str_6 = '{}{}\n'.format('cbacacbb',' '*5)
    return [str_1, str_2, str_3, str_4, str_5, str_6]



def check(reply, clue):
    return reply.strip() == clue.strip()



def solve(dataset):
    memory = list(dataset.replace('\n', ''))
    
    q = 'q1'
    result = [q]
    index = 0
    table = {
        'q1': {
            'a': ('b', 1, 'q3'),
            'b': ('c', 1, 'q3'),
            'c': ('a', 1, 'q3'),
            ' ': (' ', 1, 'q1')
        },
        'q2': {
            'a': ('b', 1, 'q3'),
            'b': ('c', 1, 'q3'),
            'c': ('a', 1, 'q3'),
            ' ': (' ', 1, 'qT')
        },
        'q3': {
            'a': ('a', 1, 'q2'),
            'b': ('b', 1, 'q2'),
            'c': ('c', 1, 'q2'),
            ' ': (' ', 1, 'qT')
        },
    }             
    while q != 'qT':
        sym, shift, state = table[q][memory[index]]
        memory[index] = sym
        index += shift
        q = state
        result.append(q)


    return "{}\n{}".format(''.join(memory), ' '.join(result))



# Write the student code to a file prog.c

student_answer = """{{ STUDENT_ANSWER | e('py') }}"""

with open("prog.py", "w") as src:
    print(student_answer, file=src)





tests = generate()

len_tests = len(tests)

correct_count = 0

incorrect_count = 0





try:
    for k, test in enumerate(tests):
        output = subprocess.check_output(["python3", "prog.py"], input=test, universal_newlines=True)
        correct_output = solve(test)
        if check(output, correct_output):
            correct_count += 1
        else:
            incorrect_count += 1
            if k < 3 and incorrect_count == 1:  # incorrect_count  < число_тестов_для показа
                result = 'Test: {}\n'.format(test)
                result += 'Your answer: {}\n'.format(output)
                result += 'Correct: {}\n'.format(correct_output)
                print(result)

except subprocess.CalledProcessError as e:
    if e.returncode > 0:
        # Ignore non-zero positive return codes
        if e.output:
            print(e.output)
    else:
        # But negative return codes are signals - abort
        if e.output:
            print(e.output, file=sys.stderr)
        if e.returncode < 0:
            print("Task failed with signal", -e.returncode, file=sys.stderr)
        print("** Further testing aborted **", file=sys.stderr)

if correct_count == len_tests:
    print('OK')

else:
    print('Wrong answer')]]>
